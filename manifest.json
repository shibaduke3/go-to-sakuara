{
  "name": "Go to サクラ",
  "description" : "クリックするとサクラチェッカーさんの商品リンクを別タブで開きます。",
  "version": "0.0.1",
  "manifest_version": 2,
  "icons": {
    "48": "images/gotosakura_48.png",
    "128": "images/gotosakura_128.png"
  },
  "web_accessible_resources": [
    "images/*.png"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.amazon.co.jp/*"],
      "js": ["gotosakura.js"]
    }
  ]
}